{
  "name": "salesagility/suitecrm",
  "description": "SuiteCRM Application",
  "homepage": "https://suitecrm.com",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "SalesAgility Ltd"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "platform": {
      "php": "7.3.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "api-platform/api-pack": "^1.2",
    "symfony/dotenv": "5.2.*",
    "symfony/console": "5.2.*",
    "symfony/flex": "^1.6",
    "symfony/framework-bundle": "5.2.*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/orm-pack": "^v2.1",
    "symfony/security-bundle": "5.2.*",
    "symfony/yaml": "5.2.*",
    "webonyx/graphql-php": "^0.13.8"
  },
  "require-dev": {
    "api-platform/schema-generator": "^2.2",
    "codeception/aspect-mock": "^3.1",
    "codeception/codeception": "4.1.4",
    "codeception/module-asserts": "^1.1",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-rest": "^1.2",
    "codeception/module-symfony": "^1.0",
    "dama/doctrine-test-bundle": "^6.2",
    "doctrine/doctrine-fixtures-bundle": "~3.2",
    "doctrine/doctrine-migrations-bundle": "^2.1",
    "symfony/maker-bundle": "^1.14",
    "symfony/profiler-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "core/backend/",
      "App\\Module\\": "core/modules/",
      "App\\Extension\\": "extensions/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "cache:warmup": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "scrm:extension-asset-install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "src-dir": "core/backend",
    "symfony": {
      "allow-contrib": false,
      "require": "5.2.*"
    }
  },
  "bin": [
    "bin/console"
  ]
}
