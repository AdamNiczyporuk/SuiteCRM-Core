{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "SuiteCRM": {
      "root": "core/app",
      "sourceRoot": "core/app",
      "projectType": "application",
      "prefix": "scrm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "public/dist/",
            "index": "core/app/src/index.html",
            "main": "core/app/src/main.ts",
            "polyfills": "core/app/src/polyfills.ts",
            "tsConfig": "core/app/tsconfig.app.json",
            "scripts": [],
            "styles": [
              "node_modules/bootstrap-css-only/css/bootstrap.min.css",
              "core/app/themes/suite8/css/style.scss"
            ],
            "deployUrl": "dist/",
            "assets": [
              "core/app/themes"
            ]
          },
          "configurations": {
            "serve": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "deployUrl": "/",
              "assets": [
                {
                  "glob": "**/*",
                  "input": "core/app/themes",
                  "output": "public/dist/themes"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "core/app/src/environments/environment.ts",
                  "with": "core/app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "SuiteCRM:build:serve",
            "proxyConfig": "./proxy.conf.local.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "SuiteCRM:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "SuiteCRM:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "core/app/src/test.ts",
            "polyfills": "core/app/src/polyfills.ts",
            "tsConfig": "core/app/tsconfig.spec.json",
            "karmaConfig": "core/app/karma.conf.js",
            "styles": [
              "node_modules/bootstrap-css-only/css/bootstrap.min.css",
              "core/app/themes/suite8/css/style.scss"
            ],
            "scripts": [],
            "assets": [
              "core/app/themes"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "./.eslintrc.js",
            "tsConfig": "core/app/tsconfig.app.json",
            "fix": true
          }
        }
      }
    },
    "SuiteCRM-e2e": {
      "root": "core/app/e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "core/app/e2e/protractor.conf.js",
            "devServerTarget": "SuiteCRM:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "SuiteCRM:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "SuiteCRM",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
